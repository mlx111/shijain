<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.example.mapper.CityMapper">

    <resultMap id="BaseResultMap" type="org.example.domain.City">
            <id property="cityName" column="city_name" jdbcType="VARCHAR"/>
            <result property="siteLongitude" column="site_longitude" jdbcType="DOUBLE"/>
            <result property="siteLatitude" column="site_latitude" jdbcType="DOUBLE"/>
    </resultMap>

    <sql id="Base_Column_List">
        city_name,site_longitude,site_latitude
    </sql>
</mapper>
