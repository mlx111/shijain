<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.example.mapper.CityCarOverviewMapper">

    <resultMap id="BaseResultMap" type="org.example.domain.CityCarOverview">
            <result property="cityName" column="city_name" jdbcType="VARCHAR"/>
            <result property="date" column="date" jdbcType="DATE"/>
            <result property="carCategory" column="car_category" jdbcType="VARCHAR"/>
            <result property="startingCarQuantity" column="starting_car_quantity" jdbcType="INTEGER"/>
            <result property="arrivalCarQuantity" column="arrival_car_quantity" jdbcType="DECIMAL"/>
            <result property="departureCarQuantity" column="departure_car_quantity" jdbcType="DECIMAL"/>
    </resultMap>

    <sql id="Base_Column_List">
        city_name,date,car_category,
        starting_car_quantity,arrival_car_quantity,departure_car_quantity
    </sql>
</mapper>
