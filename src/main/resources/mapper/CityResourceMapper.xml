<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.example.mapper.CityResourceMapper">

    <resultMap id="BaseResultMap" type="org.example.domain.CityResource">
            <id property="date" column="date" jdbcType="DATE"/>
            <id property="cityName" column="city_name" jdbcType="VARCHAR"/>
            <id property="productCategory" column="product_category" jdbcType="VARCHAR"/>
            <result property="storage" column="storage" jdbcType="INTEGER"/>
    </resultMap>

    <sql id="Base_Column_List">
        date,city_name,product_category,
        storage
    </sql>
</mapper>
