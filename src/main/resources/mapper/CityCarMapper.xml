<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.example.mapper.CityCarMapper">

    <resultMap id="BaseResultMap" type="org.example.domain.CityCar">
            <id property="date" column="date" jdbcType="DATE"/>
            <id property="cityName" column="city_name" jdbcType="VARCHAR"/>
            <id property="carCategory" column="car_category" jdbcType="VARCHAR"/>
            <result property="num" column="num" jdbcType="INTEGER"/>
    </resultMap>

    <sql id="Base_Column_List">
        date,city_name,car_category,
        num
    </sql>
</mapper>
