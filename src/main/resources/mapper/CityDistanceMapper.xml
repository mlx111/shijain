<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.example.mapper.CityDistanceMapper">

    <resultMap id="BaseResultMap" type="org.example.domain.CityDistance">
            <id property="cityx" column="cityX" jdbcType="VARCHAR"/>
            <id property="cityy" column="cityY" jdbcType="VARCHAR"/>
            <result property="distance" column="distance" jdbcType="DOUBLE"/>
    </resultMap>

    <sql id="Base_Column_List">
        cityX,cityY,distance
    </sql>
</mapper>
